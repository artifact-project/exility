{
  "name": "@exility/storybook",
  "version": "0.1.0",
  "description": "Storybook for Exility",
  "bin": {
    "exility-storybook": "./bin/launcher.sh"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 5111",
    "build-storybook": "build-storybook",
    "ts-build": "tsc",
    "ts-clean": "git clean -f",
    "prepublish-only": "npm run test && npm run ts-build",
    "prepublish": "[ -f index.js ] || npm run prepublish-only",
    "postpublish": "npm run ts-clean"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "src/*/*.js",
    "src/*/*.d.ts"
  ],
  "author": "RubaXa <ibnRubaXa@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@storybook/addon-options": "^3.0.1",
    "@storybook/react": "^3.0.1",
    "@types/node": "^7.0.28",
    "@types/react": "^15.0.27",
    "@types/react-dom": "^15.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "typescript": "^2.4.0",
    "awesome-typescript-loader": "^3.1.3",
    "style-loader": "^0.18.2"
  },
  "devDependencies": {
    "@exility/block": "*",
    "@exility/dom": "*"
  }
}
