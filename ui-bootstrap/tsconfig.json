{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "target": "es5",
    "declaration": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "noEmitHelpers": true,
     "lib": [
      "dom",
      "es5",
      "es6",
      "es2015",
      "es2016"
    ]
  },
 "exclude": ["dist", "node_modules"]
}
