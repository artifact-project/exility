{
  "name": "@exility/parser",
  "version": "0.1.0",
  "description": "Parser for Exility language",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage"
  },
  "author": "RubaXa <tarsh@rubaxa.org>",
  "license": "MIT",
  "dependencies": {
    "skeletik": "0.6.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "jest": "^20.0.3",
    "ts-jest": "^20.0.4",
    "typescript": "^2.3.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.tests\\.ts$",
    "setupTestFrameworkScriptFile": "<rootDir>/__jest__/extensions.ts",
    "transformIgnorePatterns": [
      "node_modules/(?!@exility|skeletik)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
